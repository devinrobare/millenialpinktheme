{	
	"name": "MillenialPinkTheme",
	"type": "light",
	"window.titleBarStyle": "custom",
	"editor.inlineSuggest.enabled": true,
	"editor.overviewRulerBorder": false,
	"colors": 
	{
		"editor.background": "#ffebed",

		"titleBar.activeBackground":"#ffb8cf",
	    "titleBar.activeForeground": "#91000a",

		"activityBar.background": "#ffdbf5",
		"activityBar.foreground": "#007554",

		"sideBar.background": "#fff8ed",
		"sideBar.foreground": "#13baba",
		"sideBar.border": "#c9fffb",
		"sideBarSectionHeader.background": "#9378ff30",

		"editorGroupHeader.tabsBackground": "#ebebf7",
		"editorGroup.dropBackground": "#ff80e350",

		"tab.inactiveBackground": "#ebebf7", 

		"toolbar.hoverBackground": "#febed3",
		"toolbar.activeBackground": "#febed3",

		"textLink.activeForeground": "#b3dbff",

		"list.activeSelectionIconForeground": "#eedfc4",
		"list.activeSelectionBackground": "#c9edff",
		"list.activeSelectionForeground": "#ff7893",
		"list.inactiveSelectionBackground": "#ff80e335", 
		"list.focusBackground": "#ffa8d1",
		"list.hoverBackground": "#c9edff50",
		"list.focusOutline": "#13baba",

		"button.background": "#007554",
		"button.foreground": "#ffdbf5",

		"checkbox.border": "#007554",

		"dropdown.background": "#d8f2eb",
		"dropdown.listBackground": "#d8f2eb",
		"dropdown.border": "#9378ff",
		"dropdown.foreground": "#9378ff",

		"debugIcon.breakpointForeground": "#ff7893",
		"debugIcon.startForeground": "#ff7893",

		"editor.selectionBackground": "#ff80e390",
		"editor.selectionHighlightBackground": "#ff789375",
		"editor.wordHighlightBackground": "#ff789375",
		"editorIndentGuide.activeBackground": "#00755490",
    	"editorIndentGuide.background": "#ff789398",
		"editorCursor.background": "#ff0000",

		"editorInlayHint.background": "#ffdbf5",
		"editorInlayHint.foreground": "#ffdbf5",
		"editorInlayHint.typeBackground": "#ffb19c",
		"editorInlayHint.typeForeground": "#ffb19c",
		"editorInlayHint.parameterForeground": "#ebebf7",

		"editorOverviewRuler.border": "#ff789380",
		"editorOverviewRuler.background": "#fff8ed",
		"editorOverviewRuler.findMatchForeground": "#ff789395",
		"editorOverviewRuler.selectionHighlightForeground": "#ff80e395",
		"editorOverviewRuler.wordHighlightForeground": "#3dffdc85",
		"editorOverviewRuler.wordHighlightStrongForeground": "#9a0fd685",
		"editorBracketMatch.background": "#3dffdc30",
		"editorBracketMatch.border": "#ff80e3",
		"editorOverviewRuler.bracketMatchForeground": "#00755490",
		"editorOverviewRuler.warningForeground": "#f50a0a98",

		"input.background": "#ffdbf5",
		"input.border":"#d10d99",
		"input.foreground": "#9a0fd6",
		"inputOption.activeBackground": "#ebebf7",
		"inputOption.activeBorder": "#24a0e3",

		"minimap.background": "#ffdbf5",
		"minimap.selectionHighlight": "#ffebed",
		"minimap.warningHighlight": "#ff0000",
		"minimapSlider.background": "#9378ff55",
		"minimapSlider.hoverBackground": "#9378ff70",
		"minimapSlider.activeBackground": "#9378ff90",

		"statusBar.background": "#b3dbff",
		"statusBar.foreground": "#9a0fd6",
		"statusBar.noFolderBackground" : "#d8f2eb",
    	"statusBar.debuggingBackground": "#ff80e3",

		"scrollbar.shadow": "#d8f2eb35",
		"scrollbarSlider.background": "#9378ff45",
		"scrollbarSlider.activeBackground": "#9378ff85",
		"scrollbarSlider.hoverBackground": "#9378ff75"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ff80e3"
			}
		},
		{
			"name": "Constant Variable",
			"scope": [
				"variable.other.constant",
				"meta.definition.variable",
				"meta.var-single-variable.expr",
				"meta.var.expr"
			],
			"settings": {
				"foreground": "#f76fa5"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#3dffdc"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#007554"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#c92053"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#9a0fd6"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#9a0fd6"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#9a0fd6"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#94b30c"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#c20c27"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#9378ff"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#24a0e3"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#d97f09"
			}
		},
		{
			"name": "Python Variables",
			"scope": "source.python",
			"settings": {
				"foreground": "#d10d99"
				}
		},
		{
			"name": "C++ Variables",
			"scope": ["meta.block.cpp",
			"meta.body.function.definition.cpp",
			"meta.function.definition.cpp", "source.cpp"
			],
			"settings": {
				"foreground": "#d10d99"
				}
		},
		{
			"name": "C++ Namespaces",
			"scope": ["entity.name.namespace.cpp",
			"meta.using-namespace.cpp"
			],
			"settings": {
				"foreground": "#f50a0a"
				}
		},
		{
			"name": "C# Variables",
			"scope": ["entity.name.variable.parameter.cs",
			"source.cs"
			],
			"settings": {
				"foreground": "#d10d99"
				}
		},
		{
			"name": "Java Variables",
			"scope": ["meta.method.body.java",
			"meta.method.java",
			"meta.class.body.java",
			"meta.class.java",
			"source.java"
			],
			"settings": {
				"foreground": "#d10d99"
				}
		},
		{
			"name": "JavaScript Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#13baba"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#91000a"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#abb2ed"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#13baba"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#f50a0a"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#13baba"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#d10d99"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#24a0e3"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#ff80e3"
			}
		},
		{
			"name": "HTML: Paragraph Text",
			"scope": "text.html.derivative",
			"settings": {
				"foreground": "#9a0fd6"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#9378ff"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#d10d99"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#13baba"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#156d75"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#156d75"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"foreground": "#91000a"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#9a0fd6"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#9378ff"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#9378ff"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#d10d99"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#c20c27"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#c92053"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#94b30c"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#13baba"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#ff80e3"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#ff80e3"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#ff80e3"
			}
		}
	]
}
